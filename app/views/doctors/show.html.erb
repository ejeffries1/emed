<h1>Hello Dr. <%= current_doctor.last_name %></h1>
<br>
<br>

<h2>Patient Roster</h2>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row">1</th>
      <td>Sample name</td>
      <td>Otto</td>
      <td>@mdo</td>
    </tr>
    <%# current_doctor.patients.each do |patient|%>
    <tr>
      <th scope="row">3</th>
      <td>Larry</td>
      <td><%# patient.fullname %></td>
      <td>@twitter</td>
    </tr>
    <%# end %>
  </tbody>
</table>